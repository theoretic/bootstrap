a.icon{
	i{
	float:left;
	display:block;
	padding:0px;
	margin:0 5px 0 0;
	width:25px;
	height:25px;
	background:url(@{path}/img/sprites/icons.png) 0 0 no-repeat;
	}
}


a.icon:visited i{
color:@red !important;
}

.icon-globe i{
margin:0 0px 0 0;
}

.icon-eye-open{
background-position: -50px 0px !important;
}
